<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias type="com.youguu.open.common.pojo.IdCard" alias="IdCard"/>
        <typeAlias type="com.youguu.open.common.pojo.OpenCounterSetting" alias="OpenCounterSetting"/>
        <typeAlias type="com.youguu.open.common.pojo.OpenFlow" alias="OpenFlow"/>
        <typeAlias type="com.youguu.open.common.pojo.OpenFlowProperty" alias="OpenFlowProperty"/>
        <typeAlias type="com.youguu.open.common.pojo.OpenFlowSecu" alias="OpenFlowSecu"/>
        <typeAlias type="com.youguu.open.common.pojo.OpenFlowSecuProperty" alias="OpenFlowSecuProperty"/>
        <typeAlias type="com.youguu.open.common.pojo.Protocol" alias="Protocol"/>
        <typeAlias type="com.youguu.open.common.pojo.UserOpenInfo" alias="UserOpenInfo"/>
    </typeAliases>

    <plugins>
        <plugin interceptor="com.youguu.core.dao.multi.MuDsRoutePlugin" />
    </plugins>

    <mappers>
        <mapper resource="mybatis/idcard_mybatis.xml"/>
        <mapper resource="mybatis/open_counter_setting_mybatis.xml"/>
        <mapper resource="mybatis/open_flow_mybatis.xml"/>
        <mapper resource="mybatis/open_flow_property_mybatis.xml"/>
        <mapper resource="mybatis/open_flow_secu_mybatis.xml"/>
        <mapper resource="mybatis/open_flow_secu_property_mybatis.xml"/>
        <mapper resource="mybatis/open_protocal_mybatis.xml"/>
        <mapper resource="mybatis/user_open_info_mybatis.xml"/>
    </mappers>
</configuration>
